{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text",
      "pluginOptions": {}
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {}
    },
    "active": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {}
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {}
    },
    "origin": {
      "type": "enumeration",
      "enum": [
        "Palmata",
        "Lana",
        "Cuero",
        "Toquilla",
        "fieltro"
      ],
      "pluginOptions": {}
    },
    "isFeatured": {
      "type": "boolean",
      "pluginOptions": {}
    },
    "taste": {
      "type": "enumeration",
      "enum": [
        "Otavalo",
        "Salasaca",
        "Saraguro",
        "Natabuela",
        "Kayambi",
        "Secoya",
        "Aruchico",
        "Montubio",
        "Toquilla",
        "Chullita",
        "Chola",
        "Puruh√°"
      ],
      "pluginOptions": {}
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "medida": {
      "type": "enumeration",
      "enum": [
        "XS - 53 cm",
        "S - 55.5 cm",
        "M - 57.5 cm",
        "L - 59.5 cm",
        "XL - 61.5 cm"
      ],
      "pluginOptions": {}
    }
  }
}
